    "test": "jest",
    "prestart": "npm run -s build",
    "start": "node dist/app.js",
    "lint": "eslint 'src/**/*.js' --fix",
    "format": "prettier-eslint 'server/**/*.{js,json}' --write"
  },
  "author": "negotium",
