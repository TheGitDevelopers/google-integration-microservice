    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "nodemon": "^1.19.1",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
